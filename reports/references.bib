@article{sand_MultimodelAssessmentGlobal_2023,
  title = {A Multi-Model Assessment of the {{Global Warming Potential}} of Hydrogen},
  author = {Sand, Maria and Skeie, Ragnhild Bieltvedt and Sandstad, Marit and Krishnan, Srinath and Myhre, Gunnar and Bryant, Hannah and Derwent, Richard and Hauglustaine, Didier and Paulot, Fabien and Prather, Michael and Stevenson, David},
  date = {2023-06-07},
  journaltitle = {Communications Earth \& Environment},
  shortjournal = {Commun Earth Environ},
  volume = {4},
  number = {1},
  pages = {1--12},
