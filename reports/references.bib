@article{cooper_HydrogenEmissionsHydrogen_2022,
  title = {Hydrogen Emissions from the Hydrogen Value Chain-Emissions Profile and Impact to Global Warming},
  author = {Cooper, Jasmin and Dubey, Luke and Bakkaloglu, Semra and Hawkes, Adam},
  date = {2022-07-15},
  journaltitle = {Science of The Total Environment},
  shortjournal = {Science of The Total Environment},
  volume = {830},
  pages = {154624},
  issn = {0048-9697},
  doi = {10.1016/j.scitotenv.2022.154624},
  url = {https://doi.org/10.1016/j.scitotenv.2022.154624},
  urldate = {2024-05-10},
  abstract = {Future energy systems could rely on hydrogen (H2) to achieve decarbonisation and net-zero goals. In a similar energy landscape to natural gas, H2 emissions occur along the supply chain. It has been studied how current gas infrastructure can support H2, but there is little known about how H2 emissions affect global warming as an indirect greenhouse gas. In this work, we have estimated for the first time the potential emission profiles (g CO2eq/MJ H2,HHV) of H2 supply chains, and found that the emission rates of H2 from H2 supply chains and methane from natural gas supply are comparable, but the impact on global warming is much lower based on current estimates. This study also demonstrates the critical importance of establishing mobile H2 emission monitoring and reducing the uncertainty of short-lived H2 climate forcing so as to clearly address H2 emissions for net-zero strategies.},
  keywords = {Climate change,Greenhouse gas emissions,Hydrogen,Hydrogen emissions,Net-zero},
  file = {/Users/jpvelez/Zotero/storage/DDLZW4XD/Cooper et al. - 2022 - Hydrogen emissions from the hydrogen value chain-e.pdf;/Users/jpvelez/Zotero/storage/BLGLWSQW/S004896972201717X.html}
}

